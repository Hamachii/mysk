command /cserver:
    trigger:
        execute console command "mv create %player% normal"
        send "&cサーバーを作成しています...少し待機していてください。" to player
        wait 3 seconds
        send "&cサーバーが作成されました！ /goserver でtpできます。" to player
command /goserver:
    trigger:
        execute console command "mvtp %player% %player%"
        send "&6移動中..." to player
